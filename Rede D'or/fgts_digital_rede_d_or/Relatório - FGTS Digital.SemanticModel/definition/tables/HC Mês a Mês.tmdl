	measure '(Percapta) HC Atualizado' = ```
			
			
			VAR Resultado = CALCULATE( 
			    AVERAGE('HC Mês a Mês'[VALOR FIXO Na Prática]) / 
			    AVERAGE('HC Mês a Mês'[HC na Prática]) 
			)
			RETURN Resultado
			```
		formatString: "R$"\ #,0.00;-"R$"\ #,0.00;"R$"\ #,0.00
		lineageTag: 4dc708f9-7be2-41df-8ee2-1651e3ca6cb1

		annotation PBI_FormatHint = {"currencyCulture":"pt-BR"}

	measure '(Percapta) HC Formalizado Em Contrato' = CALCULATE(SUM('HC Mês a Mês'[VALOR FIXO Formalizado])/SUM('HC Mês a Mês'[HC Formalizado]))
		formatString: "R$"\ #,0.00;-"R$"\ #,0.00;"R$"\ #,0.00
		lineageTag: b15e33f8-c0f0-4f21-8c22-a5ce96dd3f11

		annotation PBI_FormatHint = {"currencyCulture":"pt-BR"}

	measure '(Variação) Per Capita' = IF([(Percapta) HC Atualizado] = 0, "N/A",[(Média) Per Capta (FGTS Digital) - Cartão]- [(Percapta) HC Atualizado])
		formatString: "R$"\ #,0.00;-"R$"\ #,0.00;"R$"\ #,0.00
		lineageTag: def4b0b8-fd8f-4497-9fa3-04a6f85c6f43

		annotation PBI_FormatHint = {"currencyCulture":"pt-BR"}

	measure '%Variação de Per Capita - Teste' = IF(([(Média) Per Capta (FGTS Digital) - Cartão] - [(Percapta) HC Atualizado]/[(Percapta) HC Atualizado])=0,"N/A",(([(Média) Per Capta (FGTS Digital) - Cartão]) - [(Percapta) HC Atualizado])/[(Percapta) HC Atualizado])
		formatString: 0.00%;-0.00%;0.00%
