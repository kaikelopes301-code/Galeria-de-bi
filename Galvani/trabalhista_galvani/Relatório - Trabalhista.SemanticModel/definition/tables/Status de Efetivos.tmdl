	measure _Qtd_EfetivosAtivos = ```
			
			IF(
			    ISBLANK(
			        CALCULATE(
			            COUNT('Status de Efetivos'[CPF]),
			            USERELATIONSHIP(
			                'Status de Efetivos'[Id_Contrato],
			                'Documentos em Análise'[Id_Contrato]
			            ),
			            'Status de Efetivos'[Situação] = "Ativo"
			        )
			    ),
			    0,
