	measure '(Última Data2) Contagem' = ```
			
			VAR UltimaData = 
			    CALCULATE(
			        MAX('Status de Efetivos'[Data de Ref]),
			        ALL('Status de Efetivos'),
			        USERELATIONSHIP('dCalendário'[Date],'Status de Efetivos'[Data de Ref])
			    )
			
			RETURN
			    CALCULATE(
			        [Quantidade Efetivos Ativos],
			        'Status de Efetivos'[Data de Ref]= UltimaData
			    )
			    
			```
		formatString: 0
		lineageTag: c6529c3f-ac6d-4fd0-be7d-08a607daa545

	measure '(ATIVOS) - MENOS_MESES' = ```
			
			VAR MesAtual = MONTH(TODAY())
			VAR AnoAtual = YEAR(TODAY())
			VAR DataInicio = DATE(AnoAtual, MesAtual - 2, 1)
			VAR DataFim = EOMONTH(DataInicio, 0)
			
			RETURN
			CALCULATE(
			    [_Qtd_EfetivosAtivos],
			   'Status de Efetivos'[Data de Ref]>= DataInicio &&
			   'Status de Efetivos'[Data de Ref] <= DataFim,
			    USERELATIONSHIP('dCalendário'[Date],'Status de Efetivos'[Data de Ref])
			)+0
			
			```
