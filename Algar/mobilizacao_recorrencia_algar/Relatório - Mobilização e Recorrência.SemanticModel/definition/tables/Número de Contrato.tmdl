				RETURN """" & FUNC & " (" & FORMAT(FUNC/TOTAL,"#0.0%" & ")")

	measure '(Pedidos) _Quantidade' =
			
			VAR CONTRATOS = CALCULATE(DISTINCTCOUNT('Número de Contrato'[Número do Pedido]))
			
			
